// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/build/": true,  
        "**/node_modules/": true,        
        "**/db_backup/": true,        
        "**/*.js.map": true,
        "**/*.js": {"when": "$(basename).ts"},
        "ravendb-nodejs-client":true,
        "tmp":true,
        "application.log":true,
        "GeoLite2-Country.mmdb":true,
        "poker.admin.ui/":true,
        "**/dist*/**":true,
        "poker.admin.ui.angular/dist/":true,
    },
    "workbench.editor.enablePreview": false,
    "peacock.color": "#42b883",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#1c7150",
        "activityBar.activeBorder": "#ba7fdf",
        "activityBar.background": "#1c7150",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#ba7fdf",
        "activityBarBadge.foreground": "#15202b",
        "sash.hoverBorder": "#1c7150",
        "statusBar.background": "#124833",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#1c7150",
        "statusBarItem.remoteBackground": "#124833",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#124833",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#12483399",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.remoteColor": "#124833"
}